{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": "off", //TODO: remove before prod
    "no-console": "warn",
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "only-multiline"
        }],
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": false,
        "trailingComma": "es5"
      }
    ]
  }
}
